{
  "name": "tecnickcom/tc-lib-pdf",
  "description": "LH PDF Library",
  "type": "library",
  "homepage": "http://www.github.com/lucashelal/pdf",
  "license": "CC BY-NC-ND 3.0",
  "keywords": [
    "tc-lib-pdf",
    "TCPDF",
    "PDF",
    "PDFD32000-2008",
    "document"
  ],
  "authors": [
    {
      "name": "Lucas Helal",
      "email": "lucashelal@gmail.com",
      "role": "lead"
    }
  ],
  "require": {
        "php": ">=8.0",
        "monolog/monolog": "2.3.*"
  },
  "require-dev": {
    "pdepend/pdepend": "2.13.0",
    "phpmd/phpmd": "2.13.0",
    "phpunit/phpunit": "10.1.2 || 9.6.13",
    "squizlabs/php_codesniffer": "3.7.2"
  },
  "autoload": {
    "psr-4": {
      "Com\\Tecnick\\Pdf\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test"
    }
  },
  "archive": {
    "exclude": [
      "/example"
    ]
  }
}
